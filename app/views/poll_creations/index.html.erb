<h1>Listing poll_creations</h1>

<table>
  <tr>
    <th>Poll type</th>
    <th>Category</th> 
    <th>Title</th>
    <th>Question</th>
    <th>Exp date</th>
    <th>Chart type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @polls.each do |poll_creation| %>
  <tr>
    <td><%= poll_creation.poll_type_id %></td>
    <td><%= poll_creation.category_id %></td>
    <td><%= poll_creation.title %></td>
    <td><%= poll_creation.question %></td>
    <td><%= poll_creation.exp_date %></td>
    <td><%= poll_creation.chart_type %></td>
  </tr>
<% end %>
</table>

