<h1>Titel: <%= @poll.title %></h1>
<h2>Umfrage: <%= @poll.questin %></h2>
<% if @poll.poll_type.name == "single choice"%>
	<%= render 'single_choice_form' %>
<% end %>
<% if @poll.poll_type.name == "multiple choice"  %>
	<%= render 'multiple_choice_form' %>
<% end %>
<% if @poll.poll_type.name == "open"  %>
	<%= render 'open_form' %>
<% end %>


